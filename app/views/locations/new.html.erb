<%= form_for(@location) do |f| %>

  <div class="form-group">
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </div>

  <div class="form-group">
    <%= f.label :description %><br />
    <%= f.text_field :description %>
  </div>

  <%= f.submit "Create", class: "btn btn-primary" %>

<% end %>
